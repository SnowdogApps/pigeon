{
  "version": 2,
  "builds": [
    { "src": "api/get.js", "use": "@now/node" },
    { "src": "api/post.js", "use": "@now/node" },
    { "use": "@now/static-build", "src": "package.json" }
  ],
  "routes": [
    { "src": "/api/form", "dest": "/api/get.js", "methods": ["GET"] },
    { "src": "/api/form", "dest": "/api/post.js", "methods": ["POST"] }
  ],
  "env": {
    "IS_DEV": false,
    "SERVICE": "Mailgun",
    "USER": "@mailgun-user",
    "PASS": "@mailgun-password"
  }
}
